<script>
var keepscroll=true;
var gamecnt=96;
var newGamesLoading=false;
var gtagg="new";
window.onscroll=function(){
if(keepscroll&&!newGamesLoading){
var scelemt=document.getElementById("category_games");
if((document.documentElement.scrollTop||document.body.scrollTop)>scelemt.offsetHeight+150-window.innerHeight){
newGamesLoading=true;
var xhttp = new XMLHttpRequest();
xhttp.onreadystatechange = function() {
if (this.readyState == 4 && this.status == 200) {
var response=this.responseText;
response=response.replace(")]}',","");
response=JSON.parse(response);
if(!response.errors){
gamecnt=response.cnt;
//loop/insert new games
for (var i = 0, len = response.games.length; i < len; i++) {
var gamethumbwrap = document.createElement("div");
gamethumbwrap.className="thumbWrapper newboxshadow";
var gamethumbwrap2 = document.createElement("div");
var gameThumbLink = document.createElement('a');
gameThumbLink.setAttribute('href', "https://lagged.com/en/g/"+response.games[i].url_key);
gameThumbLink.setAttribute('title', response.games[i].name);
var gameThumbLinkName=document.createTextNode(response.games[i].name);
gameThumbLink.appendChild(gameThumbLinkName);
var gameThumbThumb = document.createElement('img');
gameThumbThumb.setAttribute('src', "https://imgs2.dab3games.com/"+response.games[i].thumb);
gameThumbThumb.setAttribute('alt', response.games[i].name);
gameThumbThumb.setAttribute('width', "200");
gameThumbThumb.setAttribute('height', "200");
gamethumbwrap2.appendChild(gameThumbLink);
gamethumbwrap2.appendChild(gameThumbThumb);
var gameSpanFirst = document.createElement('span');
gameSpanFirst.className="thumbname";
var gameSpanSec = document.createElement('span');
gameSpanSec.appendChild(gameThumbLinkName);
gameSpanFirst.appendChild(gameSpanSec);
gamethumbwrap2.appendChild(gameSpanFirst);
if(response.games[i].has_scores==="1"){
var gameSpanAward1 = document.createElement('span');
gameSpanAward1.className="awards-bit";
gamethumbwrap2.appendChild(gameSpanAward1);
}
if(response.games[i].has_achs==="1"){
var gameSpanAward2 = document.createElement('span');
gameSpanAward2.className="awards-bit2";
if(response.games[i].has_scores==="1"){
gameSpanAward2.className="awards-bit2 awardbitpad";
}
gamethumbwrap2.appendChild(gameSpanAward2);
}
gamethumbwrap.appendChild(gamethumbwrap2);
category_games.appendChild(gamethumbwrap);
}
if(response.keepgoing===true){
newGamesLoading=false;
}else{
keepscroll=false;
}
}else{
keepscroll=false;
}
}
};
xhttp.open("POST", "https://lagged.com/js/v4/ajax_scroll.php", true);
xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
xhttp.send("tagid="+gtagg+"&cnt="+gamecnt);
}
}
};
</script>
